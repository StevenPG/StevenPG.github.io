body {
	margin: 0px;
	padding: 0px;
	background-image: url('background.jpg');
	background-repeat: no-repeat;
	background-size: 100% 100%;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
}
a {
	color: #076286;
	text-decoration: underline;
}
a:hover {
	color: #076286;
	text-decoration: none;
	border-radius: 10px;
}
.copyright {
	border: 0px;
	height: 1px;
	width: 1px;
}
#main-wraper {
	width: 934px;
	margin: 0 auto;
	padding: 0px;
}
#top-wraper {
	width: 934px;
	float: left;
}
#banner {
	width: 814px;
	height: 90px;
	float: left;
	padding: 65px 0px 0px 120px;
	font-family: Georgia, "Times New Roman", Times, serif;
	font-size: 40px;
	text-decoration:underline;
	color: #fff;
}
#nav {
	width: 934px;
	float: left;
	background: url(../images/nav-bg.jpg) top repeat-x;
	height: 36px;
	border-radius: 10px;
}
#nav ul {
	margin: 0px;
	padding: 0px;
	list-style-type: none;
	display: inline;
	float: left;
}
#nav ul li {
	float: left;
	line-height: 36px;
	text-align: center;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #fff;
	text-decoration: none;
	background: url(../images/nav-brd.jpg) top right no-repeat;
}
#nav ul li a {
	float: left;
	line-height: 36px;
	text-align: center;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #fff;
	text-decoration: none;
	padding: 0px 54px 0px 53px;
}
#nav ul li a:hover {
	float: left;
	line-height: 36px;
	text-align: center;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #0aa5c2;
	text-decoration: none;
	padding: 0px 54px 0px 53px;
	background: url(../images/nav-h.jpg) top repeat-x;
}
#mid-wraper {
	width: 934px;
	margin: 0 auto;
	padding: 0px;
	border-radius:10px;
}
#mid-wraper-inner {
	width: 934px;
	float: left;
	background: url(../images/mid-bg.jpg) repeat-y;
	border-radius: 10px;
}
#left-wraper {
	width: 296px;
	float: left;
	border-radius: 10px;
}
#left-wraper-top {
	width: 296px;
	background: #fef4e9 url(../images/left-mid.jpg) bottom no-repeat;
	padding: 0px 0px 12px 0px;
	float: left;
	border-radius: 10px;
}
#left-wraper-top ul {
	margin: 0px;
	padding: 0px;
	list-style-type: none;
	display: block;
	float: left;
}
#left-wraper-top ul li {
	width: 241px;
	float: left;
	line-height: 31px;
	padding: 0px 15px 0px 40px;
	background: #fdead5 url(../images/left-bullet.jpg) top left no-repeat;
	text-decoration: none;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #000;
	background-position: 25px 9px;
}
#left-wraper-top ul li a {
	color: #000;
}
#left-wraper-top ul li a:hover {
	background: #edaf7b;
	text-decoration: none;
}
#news {
	width: 270px;
	float: left;
	padding: 15px 0px 10px 15px;
	color: #fff;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
}
h3 {
	margin: 0px;
	padding: 0px;
	color: #fff;
	font-size: 18px;
	font-weight: normal;
	font-family: Arial, Helvetica, sans-serif;
}
.link {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	font-weight: bold;
	color: #fff;
	text-decoration: none;
	text-align: right;
}
.link:hover {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	font-weight: bold;
	color: #fff;
	text-decoration: none;
	text-align: right;
}
.link2 {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	font-weight: bold;
	color: #bc4105;
	text-decoration: none;
	text-align: right;
}
.link2:hover {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	font-weight: bold;
	color: #bc4105;
	text-decoration: none;
	text-align: right;
}
h1 {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 18px;
	color: #2f1a1a;
	margin: 0px;
	padding: 0px;
	text-align: left;
}
h2 {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 16px;
	color: #2f1a1a;
	margin: 0px;
	padding: 0px;
	text-align: left;
}
#right-wraper {
	width: 612px;
	float: left;
	padding:15px 0px 0px 25px;
	overflow: auto;
	height: 500px;
}
#padright {
	padding:0px 15px 0px 10px;
}
.right-part {
	float: left;
}
.blue-text {
	font-size: 15px;
	color: #0e7aa5;
}
#foter {
	width: 934px;
	margin: 0 auto;
	padding: 0px;
	color: #999999;
}
#foter-inner {
	width: 934px;
	float: left;
	padding: 10px 0px;
}
.footer-link {
	color: #999999;
	text-decoration: none;
	padding: 0px 5px;
}
.footer-link:hover {
	color: #999999;
	text-decoration: none;
	padding: 0px 5px;
}

 .form{ 
    max-width: 100%; 
    min-width: 100%; 
    border-width: 2px; 
    border-color: #CCCCCC; 
    border-radius: 4px; 
    border-style: solid; 
    color: #222222; 
    font-size: 15px; 
    margin: 0px; 
    background-color: #FFFFFF; 
    padding: 20px; 
} 
.content{ 
    margin: 0px; 
} 
.form label{ 
    color: #222222; 
    font-size: 14px; 
    display: block; 
} 
.form input[type=radio], input[type=checkbox]{ 
    margin: 10px; 
    width: 13px; 
} 
.form div{ 
    display: block; 
} 
.form input, form textarea, form select{ 
    border-width: 1px; 
    border-style: solid; 
    border-color: #666666; 
    border-radius: 0px; 
    padding: 3px; 
    width: 100%; 
} 
.form, .form h1, .form h2{ 
    font-family: 'Ubuntu', sans-serif;; 
} 
.form h1{ 
    font-size: 40px; 
    color: #607A75; 
    padding: 0px; 
    margin: 0px; 
    margin-bottom: 10px; 
    border-bottom-style: dotted; 
    border-bottom-color: #CCCCCC; 
    border-bottom-width: 2px; 
    border-radius: 0px; 
    background-color: #FFFFFF; 
} 
.intro{ 
    margin-bottom: 10px; 
} 
.clear{ 
    clear: both; 
} 
.form textarea{ 
    height: 50px; 
    width: 100%; 
} 
.form input[type=submit]{ 
    width: 100%; 
    background-color: #CCCCCC; 
    color: #222222; 
} 
.field{ 
    margin-bottom: 5px; 
} 




